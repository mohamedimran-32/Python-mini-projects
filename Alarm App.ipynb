    "import time\n",
    "\n",
    "print(time.strftime(\"%H:%M:%S\"))\n",
    "alarm_time = input(\"Enter your wake up time(HH:MM:SS):\")\n",
    "while time.strftime(\"%H:%M:%S\") != alarm_time:\n",
    "    time.sleep(1)\n",
    "print(\"Wake up !!\")"
   ]
